local player = game:GetService("Players").LocalPlayer
local Gui = game.CoreGui

local rs = game:GetService("RunService")
if rs:IsStudio() then
	Gui = game.Players.LocalPlayer:WaitForChild("PlayerGui")
end

local ChingChong = Instance.new("ScreenGui")
local Icon = Instance.new("ImageLabel")
local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")

ChingChong.Name = "ChingChong"
ChingChong.Parent = Gui
ChingChong.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Icon.Name = "Icon"
Icon.Parent = ChingChong
Icon.AnchorPoint = Vector2.new(0.5, 0.5)
Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Icon.BackgroundTransparency = 1.000
Icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
Icon.BorderSizePixel = 0
Icon.Position = UDim2.new(0.5, 0, 0.5, 0)
Icon.Size = UDim2.new(0, 0, 0, 0)
Icon.Image = "rbxassetid://91539613788151"

UIAspectRatioConstraint.Parent = Icon

local ts = game:GetService("TweenService")
ts:Create(Icon, TweenInfo.new(2), {Size = UDim2.new(0.3, 0, 0.3, 0)}):Play()

if game.PlaceId == 16732694052 then
	print("ficsh")
	
	task.spawn(function()
		task.wait(5)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ChingChongGuy/ChingChong/refs/heads/main/Scripts/Ficsh.txt"))()
	end)
else
	print("unsported game")
end

task.wait(3)
ts:Create(Icon, TweenInfo.new(2), {ImageTransparency = 1}):Play()
task.wait(2)

ChingChong:Destroy()
